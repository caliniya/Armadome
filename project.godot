; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Armadome"
run/main_scene="res://界面/常规/标题.tscn"
config/features=PackedStringArray("4.4", "Mobile")
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://素材/logo.png"
boot_splash/fullsize=false
boot_splash/image="res://素材/logo.png"

[audio]

general/ios/session_category=3

[autoload]

InputManager="*res://脚本/插件/输入管理器/InputManager.gd"
Main="*res://脚本/全局/全局字典.gd"
World="*res://脚本/全局/世界.gd"
Core="*res://脚本/全局/全局方法.gd"

[debug]

settings/stdout/print_fps=true
shapes/collision/shape_color=Color(0, 0.6, 0.701961, 0.145098)

[display]

window/size/viewport_width=1800
window/size/viewport_height=800
window/size/borderless=true
window/size/always_on_top=true
window/size/extend_to_title=true
window/stretch/mode="viewport"

[editor]

export/convert_text_resources_to_binary=false

[gui]

theme/custom_font="uid://c3y3xd1nd6w6t"
theme/default_font_antialiasing=2
theme/default_font_subpixel_positioning=2
theme/default_font_multichannel_signed_distance_field=true
theme/default_font_generate_mipmaps=true
theme/lcd_subpixel_layout=3

[input_devices]

pointing/android/enable_long_press_as_right_click=true
pointing/android/enable_pan_and_scale_gestures=true

[physics]

2d/run_on_separate_thread=true
2d/default_gravity=0.0

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
